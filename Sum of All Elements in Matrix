#include <stdio.h>

int main() {

    int matrix[3][3];

    int sum = 0;

    // Read 3x3 matrix elements

    for(int i = 0; i < 3; i++) {

        for(int j = 0; j < 3; j++) {

            scanf("%d", &matrix[i][j]);

        }

    }

    // Calculate sum of elements

    for(int i = 0; i < 3; i++) {

        for(int j = 0; j < 3; j++) {

            sum += matrix[i][j];

        }

    }

    // Print sum

    printf("%d", sum);

    return 0;

}
