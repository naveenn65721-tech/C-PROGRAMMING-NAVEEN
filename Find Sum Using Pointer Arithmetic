#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    long long arr[n];
    
    // Read array elements
    for(int i = 0; i < n; i++) {
        scanf("%lld", &arr[i]);
    }

    long long sum = 0;
    long long *ptr = arr;  // Pointer to the first element

    // Calculate sum using pointer arithmetic
    for(int i = 0; i < n; i++) {
        sum += *(ptr + i);
    }

    printf("%lld\n", sum);

    return 0;
}
