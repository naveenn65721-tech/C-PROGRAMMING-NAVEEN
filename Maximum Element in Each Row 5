#include <stdio.h>

int main() {
    int n, m;

    // Read number of rows and columns
    scanf("%d %d", &n, &m);

    int matrix[n][m];

    // Read matrix elements
    for(int i = 0; i < n; i++) {
        for(int j = 0; j < m; j++) {
            scanf("%d", &matrix[i][j]);
        }
    }

    // Find and print maximum element in each row
    for(int i = 0; i < n; i++) {
        int max = matrix[i][0];  // Assume first element is max
        
        for(int j = 1; j < m; j++) {
            if(matrix[i][j] > max) {
                max = matrix[i][j];
            }
        }
        
        printf("%d\n", max);
    }

    return 0;
}
